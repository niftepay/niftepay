{
    "name": "niftepay/niftepay",
    "description": "NiftePay Payment Gateway Package for Laravel 10",
    "type": "library",
    "require": {
        "php": "^8.1",
        "guzzle/guzzle": "^7.5.0",
        "guzzlehttp/promises": "^1.5.2",
        "guzzlehttp/psr7": "^2.4.5",
        "psr/http-client": "1.0.1",
        "psr/http-factory": "^1.0.1",
        "psr/http-message": "^1.0.1",
        "ralouphie/getallheaders": "3.0.3",
        "symfony/deprecation-contracts": "3.2.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "niftepay\\niftepay\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hamadan Shafiq",
            "email": "superlogic.dev@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "niftepay\\niftepay\\provider\\ServiceProvider"
            ],
            "aliases": {
                "niftepay": "niftepay\\niftepay\\facade\\niftepayFacade"
            }
        }
    },
    "minimum-stability": "beta"
}
